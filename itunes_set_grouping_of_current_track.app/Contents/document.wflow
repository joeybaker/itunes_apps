				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}		tell application "iTunes"		set current_track to the current track		set the_grouping to the grouping of current_track	end tell		set the_new_grouping to text returned of (display dialog "grouping?" default answer the_grouping buttons {"Cancel", "Edit"} default button 2)		tell application "iTunes"		set the grouping of current_track to the_new_grouping	end tell		return the_new_groupingend run</string>
